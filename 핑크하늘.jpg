                    if(result){
                        if(flag == 1){
                            $selecotr.parents('.entry-visible').find('.cultural-agency button').attr('data-flag', 0).css("background-color","white");
                            $selecotr.parents('.entry-visible').find('.cultural-agency button i').css("color", "black");
                        }else{
                            $selecotr.parents('.entry-visible').find('.cultural-agency button').attr('data-flag', 1).css("background-color","red");
                            $selecotr.parents('.entry-visible').find('.cultural-agency button i').css("color", "white");
                        }
                    }
                }
            });
        });
        $('#grid-wrap').on('click', '.quality-control button', function() {
            var code = $(this).attr('data-target'),
                quality = $(this).attr('data-quality');
            var $selecotr = $(this);

            $.ajax({
                method: "POST",
                url: "/controller/photo/quality_control.php",
                data: {
                    code: code,
                    quality: quality
                },
                dataType: 'json',
                success: function(data){
                    var result = data.result;
                    if(result){
                        if(quality != 3){
                            $selecotr.parents('.entry-visible').find('.quality-control button').css("background-color","white");
                            $selecotr.parents('.entry-visible').find('.quality-control button i').css("color", "black");
                            $selecotr.find('i').css("color","white");
                            if(quality == 1){
                                $selecotr.css("background-color", "red");
                            }
                            if(quality == 5){
                                $selecotr.css("background-color", "blue");
                            }
                            if(quality == 6){
                                $selecotr.css("background-color", "red");
                            }
                        }else{
                            $selecotr.parents('.entry-visible').find('.quality-control button').css("background-color","white");
                            $selecotr.parents('.entry-visible').find('.quality-control button i').css("color", "black");
                        }
                    }
                }
            });
        });


        var keyword = $('[name="hidden_keyword"]').val();
        $('.top-search [name="q"]').val(keyword);
        $(window).scroll(bindScroll);
        $(document.body).on('touchmove', onScroll); // for mobile
        $('[data-toggle="tooltip"]').tooltip();
    });
</script>

